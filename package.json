    "@vue/eslint-config-prettier": "^8.0.0",
    "antd": "^5.24.6",
    "cross-env": "^7.0.3",
    "electron": "^25.6.0",
    "electron-builder": "^24.6.3",
    "electron-updater": "^6.1.7",
    "electron-vite": "^1.0.27",
    "eslint": "^8.47.0",
    "eslint-plugin-vue": "^9.17.0",
    "immer": "^10.1.1",
    "less": "^4.2.0",
    "prettier": "^3.0.2",
    "react": "^18.2.0",
