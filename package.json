    "@vicons/ionicons5": "^0.12.0",
    "ant-design-vue": "^4.1.0",
    "axios": "^1.6.7",
    "dayjs": "^1.11.13",
    "electron-log": "^5.0.3",
    "electron-store": "^8.1.0",
