{
  "name": "electron-demo",
  "version": "1.1.27",
  "description": "An Electron application with Vue",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "preview": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev:react": "electron-vite dev --config electron.vite.react.config.js",
    "dev:debug": "cross-env DEBUG=true npm run dev",
    "build": "electron-vite build",
    "build:react": "electron-vite build --config electron.vite.react.config.js",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:win:react": "npm run build:react && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:mac:react": "npm run build:react && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "build:linux:react": "npm run build:react && electron-builder --linux --config"
  },
  "dependencies": {
