    "js-cookie": "^3.0.5",
    "jspdf": "^3.0.1",
    "katex": "^0.16.22",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "markdown-it": "^14.1.0",
    "marked": "^15.0.11",
    "naive-ui": "^2.37.3",
    "nanoid": "^5.1.5",
    "pinia": "^2.1.7",
